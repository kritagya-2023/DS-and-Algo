class Node{
  public:
  int data;
  Node * next;

  public:
  Node(int data1,Node* next1){
    data = data1;
    next = next1;
  }
  public:
  Node(int data2){
    data = data2;
    next = nullptr;
  }
};



//How to initialize something 
//int a = 20;
//Node * head = new Node(2);
//to print data, cout<<head->data;
